    private void jTable1MouseClicked(java.awt.event.MouseEvent evt) {                                     
             DefaultTableModel model = (DefaultTableModel)jTable1.getModel();
             int selectedRowIndex = jTable1.getSelectedRow();
             
             jTextField4E.setText(model.getValueAt(selectedRowIndex, 0).toString());
             jTextField5N.setText(model.getValueAt(selectedRowIndex, 1).toString());
             jTextFieldS.setText(model.getValueAt(selectedRowIndex, 2).toString());
             jTextField2P.setText(model.getValueAt(selectedRowIndex, 3).toString());
             jTextField3A.setText(model.getValueAt(selectedRowIndex, 4).toString());
    }                                    

    private void jButton3DActionPerformed(java.awt.event.ActionEvent evt) {                                          
              DefaultTableModel model = (DefaultTableModel)jTable1.getModel();
              // get selected row index
              try{
              int SelectedRowIndex = jTable1.getSelectedRow();
              model.removeRow(SelectedRowIndex);
              }catch(Exception ex)
              {
                  JOptionPane.showInternalMessageDialog(null, ex);
              }    
    }                                         

    private void jTextField1KeyReleased(java.awt.event.KeyEvent evt) {                                        
        // TODO add your handling code here:
        DefaultTableModel obj=(DefaultTableModel) jTable1.getModel();
        TableRowSorter<DefaultTableModel> obj1=new TableRowSorter<>(obj);
        jTable1.setRowSorter(obj1);
        obj1.setRowFilter(RowFilter.regexFilter(jTextField1.getText()));
        
       
        
      
    }                                       

    private void jTextFieldSActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:
    }                                           

