    }                                            

    private void AddActionPerformed(java.awt.event.ActionEvent evt) {                                    
             DefaultTableModel model = (DefaultTableModel)jTable1.getModel();
             model.addRow(new Object[]{jTextField4E.getText(), jTextField5N.getText(),
                                       jTextFieldS.getText(), jTextField2P.getText(),
                                       jTextField3A.getText()});
             
    }                                   

