    private void jtuserActionPerformed(java.awt.event.ActionEvent evt) {                                       
        
       
    }                                      

    private void bloginActionPerformed(java.awt.event.ActionEvent evt) {                                       
     if (jtuser.getText().equals("")){
         JOptionPane.showMessageDialog(null, "please fill out username");
     }
     else if (jPasswordField1.getText().equals("")){
         JOptionPane.showMessageDialog(null, "please fill out password");
         
     }
     else if(jtuser.getText().contains("manueliii1")&& jPasswordField1.getText().contains("man123")){
          JOptionPane.showMessageDialog(null, "Login Successful");
           EmployeeDetails EmployeeDetailsFrame = new EmployeeDetails();
      EmployeeDetailsFrame.setVisible(true);
      EmployeeDetailsFrame.pack();
      EmployeeDetailsFrame.setLocationRelativeTo(null);
      this.dispose();
     }
     else{  
         JOptionPane.showMessageDialog(null, "Wrong Username or Password!!!","Message",JOptionPane.ERROR_MESSAGE);
     }
        
       
      
       

       
    }                                      

    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
       EmployeeDetails EmployeeDetailsFrame = new EmployeeDetails();
      EmployeeDetailsFrame.setVisible(true);
      EmployeeDetailsFrame.pack();
      EmployeeDetailsFrame.setLocationRelativeTo(null);
      this.dispose();
    }                                        

