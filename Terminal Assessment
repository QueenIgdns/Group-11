  int bsal,allowance,OT_rate,OT_hour,OT_amount,nsal;
   
   
    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
       bsal = Integer.parseInt(txtsal.getText());
        
        //Calculating Bonus
        
        if(bsal >=90000)
           allowance = bsal * 10/100;
        else if(bsal >=50000)
            allowance = bsal * 5/100;
        else
           allowance = bsal * 2/100;
        
        txtallowance.setText(String.valueOf(allowance));
        
        //Calculating OT Amount
        
        OT_rate = Integer.parseInt(txtrate.getText());
        OT_hour = Integer.parseInt(txthr.getText());
        OT_amount =  OT_rate *  OT_hour;
        
        txtamount.setText(String.valueOf(OT_amount));
        
        
        //Calculating OT NetSalary
        
        nsal = bsal + allowance + OT_amount;
        
        txtnsal.setText(String.valueOf(nsal));
        
            
    }                                        

