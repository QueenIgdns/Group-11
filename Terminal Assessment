    private void jButton6ActionPerformed(java.awt.event.ActionEvent evt) {                                         
       int i = jTable1.getSelectedRow();
       DefaultTableModel model = (DefaultTableModel)jTable1.getModel();
       
       if(i >= 0)
       {
           
          model.setValueAt(jTextField4E.getText(), i, 0);
          model.setValueAt(jTextField5N.getText(), i, 1);
          model.setValueAt(jTextFieldS.getText(), i, 2);
          model.setValueAt(jTextField2P.getText(), i, 3);
          model.setValueAt(jTextField3A.getText(), i, 4);
       }else{
           JOptionPane.showMessageDialog(null, "Error");
       }
    }                                        

